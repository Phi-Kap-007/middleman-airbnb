<ul class="list-inline text-center">
  <% data.users.each do |user| %>
    <li>
      <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle" %>
      <h3><%= user.name.split(" ").first.capitalize %> <%= user.name.split(" ").last.capitalize %></h3>
      <h3 class="user_card" ><%= user.github_nickname %> </h3>
    </li>
  <% end %>
</ul>
